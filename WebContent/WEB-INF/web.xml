<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>laiquche</display-name>
  <filter>
      <filter-name>EncodingFilter</filter-name>
      <filter-class>com.ynlqc.web.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
      <filter-name>EncodingFilter</filter-name>
      <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>UserServlet</display-name>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.ynlqc.web.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>IndexServlet</display-name>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.ynlqc.web.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CategoryServlet</display-name>
    <servlet-name>CategoryServlet</servlet-name>
    <servlet-class>com.ynlqc.web.servlet.CategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoryServlet</servlet-name>
    <url-pattern>/category</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BrandServlet</display-name>
    <servlet-name>BrandServlet</servlet-name>
    <servlet-class>com.ynlqc.web.servlet.BrandServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BrandServlet</servlet-name>
    <url-pattern>/brand</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SeriesServlet</display-name>
    <servlet-name>SeriesServlet</servlet-name>
    <servlet-class>com.ynlqc.web.servlet.SeriesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SeriesServlet</servlet-name>
    <url-pattern>/series</url-pattern>
  </servlet-mapping>
</web-app>